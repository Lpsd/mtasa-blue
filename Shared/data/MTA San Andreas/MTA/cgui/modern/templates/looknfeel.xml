                <Image type="LeftEdge" imageset="CGUI-Images" image="FrameLeft" />
                <Image type="TopEdge" imageset="CGUI-Images" image="FrameTop" />
                <Image type="RightEdge" imageset="CGUI-Images" image="FrameRight" />
                <Image type="BottomEdge" imageset="CGUI-Images" image="FrameBottom" />
                <Image type="Background" imageset="CGUI-Images" image="FrameBackground" />
              <Colours topLeft="FF202020" topRight="FF202020" bottomLeft="FF202020" bottomRight="FF202020" />
            </FrameComponent>
        </ImagerySection>
